<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ikram.cakeshop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="id.ikram.cakeshop.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="id.ikram.cakeshop.OrderActivity" />
        <activity android:name="id.ikram.cakeshop.ProfileActivity" />
        <activity android:name="id.ikram.cakeshop.TopMenuActivity"></activity>
    </application>

</manifest>